.container-fluid
  #map
    %a.map-overlay.participate{href: 'https://github.com/InnoZ/RubyCars.git'}
    %a
    .col-xs-3.col-lg-2.provider-button-container.map-overlay
      = render 'links'
    .map-overlay.map-theme-switcher-container
